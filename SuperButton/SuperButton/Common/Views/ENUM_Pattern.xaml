<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d"
	xmlns:local="clr-namespace:SuperButton"
	x:Class="SuperButton.ENUM_Pattern"
	d:DesignWidth="640" d:DesignHeight="480">
	<UserControl.Resources>
		<local:ENUM_PatternModel x:Key="ENUM_PatternModelDataSource" />
	</UserControl.Resources>

	<Grid x:Name="LayoutRoot" DataContext="{Binding Source={StaticResource ENUM_PatternModelDataSource}}"/>
</UserControl>
